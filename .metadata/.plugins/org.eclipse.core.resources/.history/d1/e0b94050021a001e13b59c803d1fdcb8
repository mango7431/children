import lombok.extern.slf4j.Slf4j;

@Slf4j
@Controller
public class RoomController {
	
	@RequestMapping(value = "/roomSelectAll.do", method = RequestMethod.GET)
	public String roomSelectAll() {
		log.info("/roomSelectAll.do");
		
		return "room/selectAll";
	}

}
