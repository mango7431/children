<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="test.com.idle">

	<insert id="BLACK_INSERT" parameterType="test.com.idle.vo.BlackVO">
		insert into black(black_type,reporter_id,target_id,board_num,comments,black_category)
		value(seq_black.nextval,#{black_type},#{reporter_id},#{target_id},#{board_num},#{comments},#{black_category})
		where board.board_num=#{board_num}
	</insert>
</mapper>